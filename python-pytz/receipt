# SliTaz package receipt.

PACKAGE="python-pytz"
VERSION="2018.3"
CATEGORY="development"
SHORT_DESC="World Timezone Definitions for Python"
MAINTAINER="pascal.bellard@slitaz.org"
LICENSE="MIT"
WEB_SITE="https://pypi.python.org/pypi/pytz"

BUILD_DEPENDS="python"

compile_rules() {
	pip install --no-compile --root=$DESTDIR pytz
}

genpkg_rules() {
	copy @std
	DEPENDS="python"
}
