# SliTaz package receipt v2.

PACKAGE="cmospwd"
VERSION="5.0"
CATEGORY="misc"
SHORT_DESC="Decrypts password stored in cmos used to access BIOS SETUP"
MAINTAINER="pascal.bellard@slitaz.org"
LICENSE="GPL2"
WEB_SITE="http://www.cgsecurity.org/wiki/CmosPwd"

TARBALL="$PACKAGE-$VERSION.tar.bz2"
WGET_URL="http://www.cgsecurity.org/$TARBALL"

compile_rules() {
	cd $src/src
	rm -f cmospwd
	make &&
	install -Dm755 $src/src/cmospwd $install/usr/bin/cmospwd
}

genpkg_rules() {
	copy @std
	TAGS="CMOS"
}
