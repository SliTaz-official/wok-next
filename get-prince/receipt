# SliTaz package receipt v2.

PACKAGE="get-prince"
VERSION="1.00"
CATEGORY="non-free"
SHORT_DESC="Formatter for converting XML and HTML into PDF"
MAINTAINER="devel@slitaz.org"
LICENSE="BSD"
WEB_SITE="https://www.princexml.com/"
REPOLOGY="-"

compile_rules() {
	install -Dm755 $stuff/$PACKAGE $install/usr/bin/$PACKAGE
}

genpkg_rules() {
	copy @std
	TAGS="utilities"
}
