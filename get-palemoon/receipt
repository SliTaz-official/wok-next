# SliTaz package receipt v2.

PACKAGE="get-palemoon"
VERSION="27.6.1"
CATEGORY="non-free"
SHORT_DESC="Browser based on Firefox that's optimised for performance. Official sse2 version."
MAINTAINER="al.bobylev@gmail.com"
LICENSE="GPL"
WEB_SITE="http://www.palemoon.org/"
REPOLOGY="-"

compile_rules() {
	install -Dm0755 $stuff/get-palemoon $install/usr/bin/get-palemoon
}

genpkg_rules() {
	copy @std
	TAGS="browser"
}

post_install() {
	echo 'To get browser run get-palemoon command as root'
	echo 'For media playback try to install ffmpeg package'
}

