# SliTaz package receipt v2.

PACKAGE="jdupes"
VERSION="1.10.2"
CATEGORY="development"
SHORT_DESC="A powerful duplicate file finder and an enhanced fork of 'fdupes'"
MAINTAINER="al.bobylev@gmail.com"
LICENSE="MIT"
WEB_SITE="https://github.com/jbruchon/jdupes/"

TARBALL="$PACKAGE-$VERSION.tar.gz"
WGET_URL="https://github.com/jbruchon/jdupes/archive/v$VERSION.tar.gz"

compile_rules() {
	make &&
	make install
}

genpkg_rules() {
	copy @std
}
