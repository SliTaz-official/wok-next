# SliTaz package receipt v2.

PACKAGE="poppler-data"
VERSION="0.4.9"
CATEGORY="misc"
SHORT_DESC="Encoding files for use with poppler"
MAINTAINER="rocky@slitaz.org"
LICENSE="MIT"
WEB_SITE="https://poppler.freedesktop.org/"
LFS="http://www.linuxfromscratch.org/blfs/view/stable/general/poppler.html"

TARBALL="$PACKAGE-$VERSION.tar.gz"
WGET_URL="http://poppler.freedesktop.org/$TARBALL"

compile_rules() {
	make prefix=/usr install
}

genpkg_rules() {
	copy @std @dev
}
