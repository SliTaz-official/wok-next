# SliTaz package receipt.

PACKAGE="gst0-python-dev"
VERSION="0.10.21"
SERIES="0.10"
CATEGORY="development"
SHORT_DESC="gst0-python devel files"
MAINTAINER="jozee@slitaz.org"
LICENSE="LGPL2.1"
WANTED="gst0-python"
WEB_SITE="http://gstreamer.freedesktop.org/"

DEPENDS="pygobject-dev gstreamer0-dev gst0-plugins-base-dev python-dev pkg-config"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	mkdir -p $fs/usr/lib
	cp -a $install/usr/lib/pkgconfig $fs/usr/lib
}
