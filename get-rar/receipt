# SliTaz package receipt.

PACKAGE="get-rar"
VERSION="171201"
CATEGORY="non-free"
SHORT_DESC="Get RAR for Linux"
MAINTAINER="pascal.bellard@slitaz.org"
LICENSE="BSD"
WEB_SITE="https://www.rarlab.com/"
TAGS="compression archive"

compile_rules() {
	install -Dm0755 $stuff/get-rar $install/usr/bin/get-rar
}

genpkg_rules() {
	copy @std
}

