# SliTaz package receipt v2.

PACKAGE="get-rar"
VERSION="171201"
CATEGORY="non-free"
SHORT_DESC="Get RAR for Linux"
MAINTAINER="pascal.bellard@slitaz.org"
LICENSE="BSD"
WEB_SITE="https://www.rarlab.com/"
REPOLOGY="-"

compile_rules() {
	install -Dm755 $stuff/$PACKAGE $install/usr/bin/$PACKAGE
}

genpkg_rules() {
	copy @std
	TAGS="compression archive"
}
