# SliTaz package receipt.

PACKAGE="linux64-dialup"
VERSION="4.9.0"
CATEGORY="base-system"
SHORT_DESC="The Linux Kernel dial-up modules"
MAINTAINER="domcox@slitaz.org"
LICENSE="GPL2"
WEB_SITE="https://www.kernel.org/"
PROVIDE="linux-dialup:linux64"

WANTED="linux"
DEPENDS="linux64"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	. $wanted_stuff/copy_modules.sh
	copy_modules \
		drivers/tty/serial/serial_cs.ko.xz \
		drivers/usb/serial \
		drivers/net/usb/hso.ko.xz
}
