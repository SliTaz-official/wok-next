# SliTaz package receipt.

PACKAGE="curl-dev"
VERSION="7.50.1"
CATEGORY="development"
SHORT_DESC="Tool and libs for transferring files with URL syntax."
MAINTAINER="pankso@slitaz.org"
LICENSE="MIT"
WEB_SITE="https://curl.haxx.se/"
HOST_ARCH="i486 arm"

WANTED="curl"
DEPENDS="curl libidn-dev openssl-dev libcrypto-dev zlib-dev"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	cook_copy_files curl-config *.h *.la *.pc *.m4
}
