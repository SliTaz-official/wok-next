# SliTaz package receipt.

PACKAGE="linux64-configfs"
VERSION="4.9.0"
CATEGORY="base-system"
SHORT_DESC="The Linux Kernel ConfigFs module"
MAINTAINER="devel@slitaz.org"
LICENSE="GPL2"
WEB_SITE="https://www.kernel.org/"
CONFIG_FILES="/etc/filesystems"
PROVIDE="linux-configfs:linux64"

WANTED="linux"
DEPENDS="linux64"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	. $wanted_stuff/copy_modules.sh
	copy_modules fs/configfs
}
