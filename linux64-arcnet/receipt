# SliTaz package receipt.

PACKAGE="linux64-arcnet"
VERSION="4.9.0"
CATEGORY="base-system"
SHORT_DESC="The Linux Kernel ARCnet modules"
MAINTAINER="slaxemulator@gmail.com"
LICENSE="GPL2"
WEB_SITE="https://kernel.org/"
PROVIDE="linux-arcnet:linux64"

WANTED="linux"
DEPENDS="linux64"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	. $wanted_stuff/copy_modules.sh
	copy_modules \
		drivers/net/arcnet \
		drivers/net/arcnet/arcnet.ko.xz \
		drivers/net/arcnet/com90xx.ko.xz \
		drivers/net/arcnet/rfc1051.ko.xz
}
