# SliTaz package receipt.

PACKAGE="linux-ocfs2"
VERSION="4.9.0"
CATEGORY="base-system"
SHORT_DESC="The Linux Kernel OCFS2 modules"
MAINTAINER="slaxemulator@gmail.com"
LICENSE="GPL2"
WEB_SITE="http://kernel.org/"
CONFIG_FILES="/etc/filesystems"

WANTED="linux"
DEPENDS="linux"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	. $wanted_stuff/copy_modules.sh
	copy_modules fs/ocfs2
}
