# SliTaz package receipt v2.

PACKAGE="python-pyasn1"
VERSION="latest"
CATEGORY="development"
SHORT_DESC="ASN.1 types and codecs"
MAINTAINER="pascal.bellard@slitaz.org"
LICENSE="BSD"
WEB_SITE="https://pypi.org/project/pyasn1/"
HOST_ARCH="any"
REPOLOGY="python:pyasn1"

BUILD_DEPENDS="python"

compile_rules() {
	pip install --no-compile --root=$DESTDIR pyasn1
}

genpkg_rules() {
	VERSION=$(sed -n '/^Successfully installed/ s|.*pyasn1-||p' $LOGS/$PACKAGE.log)
	copy @std
	DEPENDS="python"
}
