# SliTaz package receipt v2.

PACKAGE="get-wakoopa"
VERSION="1.02"
CATEGORY="non-free"
SHORT_DESC="Perfect place to discover software and games online"
MAINTAINER="devel@slitaz.org"
LICENSE="BSD"
WEB_SITE="http://wakoopa.com/"
REPOLOGY="-"

compile_rules() {
	install -Dm755 $stuff/$PACKAGE $install/usr/bin/$PACKAGE
}

genpkg_rules() {
	copy @std
	TAGS="games"
}
