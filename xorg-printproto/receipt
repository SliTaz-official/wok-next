# SliTaz package receipt v2.

PACKAGE="xorg-printproto"
VERSION="1.0.5"
CATEGORY="development"
SHORT_DESC="Print extension headers"
MAINTAINER="rcx@zoominternet.net"
LICENSE="MIT"
WEB_SITE="https://www.x.org/wiki/"

TARBALL="printproto-$VERSION.tar.bz2"
WGET_URL="$XORG_MIRROR/proto/$TARBALL"

BUILD_DEPENDS="xorg-util-macros"
PKG_RULE="dev"

compile_rules() {
	./configure $CONFIGURE_ARGS &&
	make &&
	make install
}

genpkg_rules() {
	copy @dev
	DEPENDS="xorg-fontsproto xorg-libX11-dev xorg-libXau-dev"
}
