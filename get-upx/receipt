# SliTaz package receipt v2.

PACKAGE="get-upx"
VERSION="1.00"
CATEGORY="non-free"
SHORT_DESC="Get UPX executable packer"
MAINTAINER="pascal.bellard@slitaz.org"
LICENSE="BSD"
WEB_SITE="https://upx.github.io/"
REPOLOGY="-"

compile_rules() {
	install -Dm755 $stuff/$PACKAGE $install/usr/bin/$PACKAGE
}

genpkg_rules() {
	copy @std
}
