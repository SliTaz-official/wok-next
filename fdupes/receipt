# SliTaz package receipt v2.

PACKAGE="fdupes"
VERSION="1.6.1"
CATEGORY="misc"
SHORT_DESC="Identify or delete duplicate files in specified directories"
MAINTAINER="pascal.bellard@slitaz.org"
LICENSE="MIT"
WEB_SITE="https://github.com/adrianlopezroche/fdupes"

TARBALL="$PACKAGE-$VERSION.tar.gz"
WGET_URL="https://github.com/adrianlopezroche/fdupes/archive/v$VERSION.tar.gz"
TARBALL_SHA1="e6c26e2012927ebecab33f00280cabbb0aef702d"

compile_rules() {
	make &&
	make PREFIX=/usr install
}
