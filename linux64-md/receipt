# SliTaz package receipt.

PACKAGE="linux64-md"
VERSION="4.9.0"
CATEGORY="base-system"
SHORT_DESC="The Linux Kernel MD modules"
MAINTAINER="devel@slitaz.org"
LICENSE="GPL2"
WEB_SITE="https://www.kernel.org/"
PROVIDE="linux-md:linux64"

WANTED="linux"
DEPENDS="linux-crypto"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	. $wanted_stuff/copy_modules.sh
	copy_modules drivers/md

	# remove duplicates
	cook linux-crypto >/dev/null 2>&1
	cook_split_rm linux-crypto
}
