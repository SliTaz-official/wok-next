# SliTaz package receipt.

PACKAGE="libxml2-dev"
VERSION="2.9.4"
CATEGORY="development"
SHORT_DESC="XML C parser and toolkit devel files."
MAINTAINER="pankso@slitaz.org"
LICENSE="MIT"
WEB_SITE="http://xmlsoft.org/"
HOST_ARCH="i486 arm"

WANTED="libxml2"
DEPENDS="libxml2-tools pkg-config   zlib-dev liblzma-dev"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	cook_copy_files xml2-config *.h *.cmake *.pc libxml2.la *.sh *.m4
}
