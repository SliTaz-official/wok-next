# SliTaz package receipt.

PACKAGE="linux64-mmc"
VERSION="4.9.0"
CATEGORY="base-system"
SHORT_DESC="The Linux Kernel MMC modules (card reader)"
MAINTAINER="devel@slitaz.org"
LICENSE="GPL2"
WEB_SITE="https://www.kernel.org/"
PROVIDE="linux-mmc:linux64"

WANTED="linux"
DEPENDS="linux64"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	. $wanted_stuff/copy_modules.sh
	copy_modules \
		drivers/mmc \
		drivers/misc/tifm_7xx1.ko.xz \
		drivers/misc/iwmc3200top/iwmc3200top.ko.xz
}
