# SliTaz package receipt v2.

PACKAGE="get-unace"
VERSION="1.00"
CATEGORY="non-free"
SHORT_DESC="Get UNACE unpacker"
MAINTAINER="pascal.bellard@slitaz.org"
LICENSE="BSD"
WEB_SITE="http://www.winace.com/"
REPOLOGY="-"

compile_rules() {
	install -Dm755 $stuff/$PACKAGE $install/usr/bin/$PACKAGE
}

genpkg_rules() {
	copy @std
	TAGS="compression archive"
}
